{
  "name": "@web-components-monorepo-starter/hello-world",
  "version": "0.0.0",
  "private": true,
  "description": "A web component with TypeScript and Lit",
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "type": "module",
  "main": "dist/hello-world.js",
  "module": "dist/hello-world.js",
  "types": "dist/hello-world.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "analyze": "custom-elements-manifest analyze --litelement --globs \"src/**/*.ts\" --outdir dist",
    "analyze:watch": "custom-elements-manifest analyze --litelement --globs \"src/**/*.ts\" --outdir dist --watch",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint --fix .",
    "lint:lit-analyzer": "lit-analyzer",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "web-test-runner",
    "test:dev:watch": "web-test-runner --watch",
    "test:prod": "MODE=prod web-test-runner",
    "test:prod:watch": "MODE=prod web-test-runner --watch",
    "type-check": "tsc --project tsconfig.quality.json"
  },
  "dependencies": {
    "lit": "^3.1.2"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.9.2",
    "@open-wc/testing": "^4.0.0",
    "@types/mocha": "^10.0.6",
    "@web-components-monorepo-starter/base-configs": "*",
    "@web/dev-server": "^0.4.2",
    "@web/dev-server-esbuild": "^1.0.2",
    "@web/test-runner": "^0.18.0",
    "lit-analyzer": "^2.0.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "customElements": "dist/custom-elements.json"
}
